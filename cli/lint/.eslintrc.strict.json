{
  "extends": ["./.eslintrc.base.json"],
  "plugins": ["@typescript-eslint", "jest", "import", "sort-keys-fix", "unused-imports"],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used" }
    ]
  }
}
